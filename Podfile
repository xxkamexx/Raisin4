  # Pods for ChatTestApp

  target 'RaisinTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'JSQMessagesViewController'
  end

  target 'RaisinUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'JSQMessagesViewController'
  end

end
